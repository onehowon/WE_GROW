    <assignments>
        <name>Has_Pricebook_Assign</name>
        <label>가격표 적용</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Var_PricebookId</assignToReference>
