            <targetReference>Get_RecordType_Space</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Has_Pricebook_Assign_2</name>
        <label>가격표 적용 2</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Var_PricebookId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Get_Current_Quote_Context.Opportunity.Pricebook2Id</elementReference>
