    <assignments>
        <name>Assign_Values_to_Temp</name>
        <label>견적 품목 변수 매핑</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>Var_Temp_QuoteLine.QuoteId</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>Var_Temp_QuoteLine.Asset__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Loop_Selected_Assets.Id</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>Var_Temp_QuoteLine.Product2Id</assignToReference>
